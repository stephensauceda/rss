{
  "name": "rss.stephensauceda.com",
  "alias": "rss.stephensauceda.com",
  "version": 2,
  "builds": [
    {
      "src": "*.js",
      "use": "@now/node"
    }
  ],
  "env": {
    "API_ENDPOINT": "@api_endpoint",
    "ACCESS_TOKEN": "@access_token"
  },
  "build": {
    "env": {
      "API_ENDPOINT": "@api_endpoint",
      "ACCESS_TOKEN": "@access_token"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/",
      "headers": {
        "cache-control": "s-maxage=7200, max-age=0"
      }
    }
  ]
}